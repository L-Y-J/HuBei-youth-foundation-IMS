<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="students" extends="struts-default">
        <action name="find_student" class="action.inner.sysprojectmanager.FindStudents" method="FindStudentBySomeThings">
        </action>
        <action name="delete_student" class="action.inner.sysprojectmanager.DeleteStudents" method="DeleteStudents">
        </action>
        <action name="add_student" class="action.inner.sysprojectmanager.AddStudents" method="AddSubsidizeStudents">
        </action>
        <action name="update_student" class="action.inner.sysprojectmanager.UpdateStudents" method="UpdateSubidizeStudents">
        </action>

        <action name="find_no_student" class="action.inner.sysprojectmanager.FindNoSubsidizeStudents" method="FindStudentsNoSubidize">
        </action>
        <action name="add_no_student" class="action.inner.sysprojectmanager.AddNoSubsidizeStudents" method="AddStudents">
        </action>
        <action name="update_no_student" class="action.inner.sysprojectmanager.UpdateNoSubsidizeStudents" method="UpdateStudents">
        </action>
    </package>

    <package name="school" extends="struts-default">
        <action name="find_school" class="action.inner.sysprojectmanager.FindSchools" method="FindSchools">
        </action>
        <action name="find_school_area" class="action.inner.sysprojectmanager.FindSchoolsArea" method="FindAllSchoolArea">
        </action>
        <action name="find_subsidizeschool_area" class="action.inner.sysprojectmanager.FindSchoolsArea" method="FindSubsidizeSchoolArea">
        </action>
        <action name="find_subsidize" class="action.inner.sysprojectmanager.LoginSchool" method="FindSubsidize">
        </action>
        <action name="find_area_qu" class="action.inner.sysprojectmanager.LoginSchool" method="FindAreaQu">
        </action>
        <action name="find_area_xian" class="action.inner.sysprojectmanager.LoginSchool" method="FindAreaXian">
        </action>
        <action name="save_writer_school" class="action.inner.sysprojectmanager.LoginSchool" method="SaveWriterAndSchoolInfo">
        </action>
    </package>

    <package name="books" extends="struts-default">
        <action name="find_req_books" class="action.inner.sysprojectmanager.FindRequestBooks" method="RequestBooks">
        </action>
        <action name="find_school_state" class="action.inner.sysprojectmanager.FindSchoolState" method="SchoolStates">
        </action>
        <action name="find_subsidize_state" class="action.inner.sysprojectmanager.FindSubsidizeState" method="SubSidizeState">
        </action>
        <action name="find_through_books" class="action.inner.sysprojectmanager.FindThroughBooks" method="ThroughBooks">
        </action>
    </package>
</struts>