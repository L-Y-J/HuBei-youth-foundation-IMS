<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="codemanager" extends="struts-default">
        <action name="department_one" class="action.inner.syscodemanager.CodeManager" method="SelectDepartmentOne">
        </action>
        <action name="department_job" class="action.inner.syscodemanager.CodeManager" method="SelectDepartmentJob">
        </action>
        <action name="add_department" class="action.inner.syscodemanager.CodeManager" method="AddDepartment">
        </action>
        <action name="del_department" class="action.inner.syscodemanager.CodeManager" method="DelDepartment">
        </action>
        <action name="add_job" class="action.inner.syscodemanager.CodeManager" method="AddJob">
        </action>
        <action name="del_job" class="action.inner.syscodemanager.CodeManager" method="DelJob">
        </action>
    </package>
</struts>